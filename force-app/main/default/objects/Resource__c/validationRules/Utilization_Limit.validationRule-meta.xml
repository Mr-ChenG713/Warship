<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Utilization_Limit</fullName>
    <active>true</active>
    <errorConditionFormula>AND( Utilization__c &gt; 1 )</errorConditionFormula>
    <errorDisplayField>Utilization__c</errorDisplayField>
    <errorMessage>Utilization between 0 to 100</errorMessage>
</ValidationRule>
